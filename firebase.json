{
    "hosting": {
        "public": "public",
        "ignore": [
            "firebase.json",
            "**/.*",
            "**/node_modules/**"
        ],
        "rewrites": [
            {
                "source": "/apfs/**",
                "destination": "/apfs/index.html"
            },
            {
                "source": "/exp/**",
                "destination": "/exp/index.html"
            },
            {
                "source": "/hdiutil/**",
                "destination": "/hdiutil/index.html"
            },
            {
                "source": "/keychain/**",
                "destination": "/keychain/index.html"
            },
            {
                "source": "/libproc/**",
                "destination": "/libproc/index.html"
            },
            {
                "source": "/mach/**",
                "destination": "/mach/index.html"
            },
            {
                "source": "/plist/**",
                "destination": "/plist/index.html"
            },
            {
                "source": "/sys/**",
                "destination": "/sys/index.html"
            }
        ]
    }
}
